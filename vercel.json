{
  "builds": [
    {
      "src": "office_emp_proj/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "build": "python3.9 office_emp_proj/manage.py collectstatic --noinput"
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "office_emp_proj/wsgi.py" }
  ]
}
